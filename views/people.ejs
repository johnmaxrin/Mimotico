<!DOCTYPE html>
<html lang="en">
<head>

    <link rel="shortcut icon" href="data:image/png;base64,"> 
    
  <title><%= resl[0].pupilname %></title>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <script src="https://kit.fontawesome.com/f09fc4a330.js"></script>
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css">
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.7/umd/popper.min.js"></script>
  <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js"></script>

<style>
@import url('https://fonts.googleapis.com/css?family=Nunito+Sans|Raleway|Roboto:300&display=swap');
 body{
  font-family: 'Roboto', sans-serif;
   
 }
</style>


</head>
<body>


    <%- include ./partials/nav.ejs %>


<div class="jumbotron"  style=" font-family: 'Raleway', sans-serif; overflow: hidden; background: -moz-linear-gradient(-45deg, rgb(244, 62, 16) 0%, rgba(137,10,255,1) 100%); color: white !important">
  <div class="container">
  	<div class="row justify-content-center">
    <div class="col-8">
    <h1 class="display-4"><%= resl[0].pupilname %>  
      
      <% console.log(resl[resl.length-1].action) %>
      <% if(resl[resl.length-1].action=='Out'){ %>
      
      <span class="badge badge-danger"> Out</span>
      <% }  else{ %>

        <span class="badge badge-success"> In</span>
        <% } %>
    </h1>
    <hr class="my-4" style="background-color: white !important">
    <p class="lead"><span class="badge badge-info"> Id </span> <%= resl[0].id %> <span class="badge badge-success"> Email </span> <%= resl[0].email %></p>
  </div>
</div>
</div>
</div>

<div class="container-fluid" >
  <div class="row justify-content-end">
       
      <div class="col-9">
      <div class="card" style="width: 70rem;">
  <div class="card-body">
    
   
    <p class="card-text">
    
    <table class="table">
  <thead>
    <tr>
      <th scope="col-1">#</th>
      <th scope="col-5">Hash</th>
      <th scope="col-5">Prev Hash</th>
      <th scope="col-4">TimeStamp</th>
      <th scope="col-1">Action</th>
      <th scope="col-3">@</th>
    </tr>
  </thead>
  <tbody>
    <tr>
        <% for(var i=0; i<resl.length; i++) { %>
      <th scope="row"><%= i+1 %></th>
      <td><%= resl[i].hash %></td>
      <td><%= resl[i].prevhash %></td>
      <td><%= resl[i].timestamp %></td>
      <td><%= resl[i].action %></td>
      <td><%= resl[i].at %></td>
    </tr>
 <% } %>
  
  </tbody>
</table>
    
    </p>
   
   
   
   
</div>
</div>
</div>
</div>
 







 

</body>
</html>
